        Game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(85.1792221, 4.57998085, 70.5875168, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        getgenv.HatchRareEgg = true 
        while getgenv().HatchEpicEgg == true do 
    local args = {
    [1] = "Rare Egg",
    [2] = true,
    [4] = true,
    [5] = {
        ["1"] = false,
        ["4"] = false,
        ["3"] = false,
        ["2"] = false}}
game:GetService("ReplicatedStorage").RemoteEvents.OpenEgg:FireServer(unpack(args))
            wait()
        end 
    else
        Game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(85.1792221, 4.57998085, 70.5875168, 1, 0, 0, 0, 1, 0, 0, 0, 1)
        getgenv.HatchRareEgg = false 
        while getgenv().HatchEpicEgg == true do 
local args = {
    [1] = "Rare Egg",
    [2] = true,
    [4] = true,
    [5] = {
        ["1"] = false,
        ["4"] = false,
        ["3"] = false,
        ["2"] = false}}
game:GetService("ReplicatedStorage").RemoteEvents.OpenEgg:FireServer(unpack(args))
            wait()
        end
    end
end)
